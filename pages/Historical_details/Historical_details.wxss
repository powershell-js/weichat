.conter{
  width: 100%;
  height: 100vh;
  overflow: hidden;
}
.conter>view{
  width: 100%;
  box-sizing: border-box;
  /* border:1rpx solid red; */
}
.title{
  height: 5%;
  display: flex;
  flex-direction: row;
  align-items:center;
  justify-content: center;
  background: #126BFC;
  color: #888;
  padding: 10rpx 0;
}
.font{
  color:#fff!important;
}
.title>view{
  padding: 0 60rpx;
  
}
.head{
  height: 5%;
  padding: 10rpx 10rpx;
  display: flex;
  flex-direction: row;
  align-items:center;
  font-size: 20rpx;
  background:rgb(221, 219, 219);
}
.head>view:nth-child(1){
  width: 20%;
  box-sizing: border-box;
  /* border:1rpx solid red; */
}
.head>view:nth-child(2){
  width: 15%;
  box-sizing: border-box;
  /* border:1rpx solid red; */
}
.head>view:nth-child(3){
  width: 25%;
  box-sizing: border-box;
  /* border:1rpx solid red; */
}
.head>view:nth-child(4){
  width: 40%;
  box-sizing: border-box;
  /* border:1rpx solid red; */
}
.contont_list{
  padding: 10rpx 0;
  display: flex;
  flex-direction: row;
  font-size: 20rpx;
}
.contont_list>view:nth-child(1){
  width: 20%;
  box-sizing: border-box;
  /* border:1rpx solid red; */
}
.contont_list>view:nth-child(2){
  width: 13%;
  box-sizing: border-box;
  /* border:1rpx solid red; */
}
.contont_list>view:nth-child(3){
  width: 27%;
  box-sizing: border-box;
  color:#126BFC;
  /* border:1rpx solid red; */
}
.contont_list>view:nth-child(4){
  width: 40%;
  box-sizing: border-box;
  /* border:1rpx solid red; */
}
.list{
  overflow: scroll;
  height: 90%;
  padding: 0 10rpx;
}