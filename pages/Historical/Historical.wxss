}
.input{
  width: 100%;
  height: 6vh;
  box-sizing: border-box;
  /* border:2rpx solid red; */
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  background: #126BFC;
}
.input>view{
  width: 40%;
  height: 60rpx;
  border:2rpx solid #fff;
  border-radius: 10rpx;
}
map{
  width: 100%;
  height: 94vh;
  box-sizing: border-box;
