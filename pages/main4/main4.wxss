.conter{
  width: 100%;
  height: 100vh;
  background: #F0F0F0;
  display: flex;
  flex-direction: column;
}
.list{
  width: 100%;
  height: 60rpx;
  background: #fff;
  margin: 20rpx 0 0 0;
  display: flex;
  flex-direction: row;
  /* align-items: center; */
  justify-content:space-between;
  text-align: center;
}
.lists{
  justify-content:center;
  color:red;
}
.dialog{
  width: 100%;
  height: 100vh;
  position: fixed;
  top:0;
  background: black;
  opacity: 0.5;
}
.dia-conter{
  text-align: center;
  z-index:99;
  left:10%;
  top:40%;
  position: absolute;
  width: 80%;
  height: 30%;
  background: #fff;
  border-radius:10rpx;
}
.test{
  margin: 48rpx 0;
}
.yes{
  border-top:2rpx solid #555;
  display: flex;
  flex-direction: row;
  justify-content:center;
}
.tishi{
  padding: 10rpx;
}
.tishi:nth-child(2){
  color: #008CFF;
}