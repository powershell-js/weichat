.map_container{
  position: absolute;
  top: 0;
  bottom: 80px;
  left: 0;
  right: 0;
}
.map{
  width: 100%;
  height: 100%;
}
.map_text{
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0px;
  height: 80px;
  background: #fff;
  padding: 0 15px;
}
text{
  margin: 5px 0;
  display: block;
  font-size:12px;
}
.h1{
  margin: 15px 0;
  font-size:15px;
}
.conter{
  width: 90%;
  height: 60rpx;
  box-sizing: border-box;
  border:2rpx solid red;
  margin: 0 auto;
  overflow-x: scroll;
}
.center{
  /* width: 1000rpx; */
  height: 50rpx;
  box-sizing: border-box;
  border:2rpx solid black;
  display: flex;
  flex-direction: row;
}
.center>view{
  width:300rpx;
  height: 50rpx;
  box-sizing: border-box;
  border:2rpx solid black;
  background: #fff;
  z-index: 9999;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.usual_progres{
  width: 100%;  
  height: 20px;  
}
