/* pages/Temperature/Temperature.wxss */
.conter{
  width: 100%;
  height: 100vh;
  box-sizing: border-box;
  /* border:1rpx solid red; */
  overflow: hidden;
}
.conter>view{
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  /* border:1rpx solid red; */
  width: 100%;
  height: 50vh;
}
.dowload{
  width: 100%;
  background: #008CFF;
}
.top{
  font-size: 24rpx;
}
.co_img{
  width: 84rpx;
  height: 84rpx;
  float: right;
}
.tem1{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  width: 80%;
  margin: 0 auto;
  box-sizing: border-box;
  /* border:1rpx solid red; */
}
.tem1_1{
  color: #75DB86;
}
.tem1_2_2{
  color: #5553CE;
}
.tem1_3{
  color: #F25850;
}
.tem1_4{
  color: #32A4FF;
}
.tem1_2{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.tem1_2>view{
  margin: 0 10rpx;
}
.echart{
  width: 100%;
  height: 75%;
  box-sizing: border-box;
  /* border:1rpx solid red; */
  overflow: hidden;
}
.list{
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  /* border:1rpx solid red; */
  overflow: hidden;
}
ec-canvas{
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  /* border:1rpx solid red; */
  overflow: hidden;
}
.data_list{
  width: 100%;
  height: 80rpx;
  box-sizing: border-box;
  /* border:1rpx solid red; */
  display: flex;
  flex-direction: row;
  line-height: 80rpx;
  background: #EBEEFF;
}
.data_list>view{
  margin-left: 30rpx;
  margin-right: 40rpx;
}
.data_list_1{
  width: 100%;
  height: 100%;
  box-sizing: border-box;
  /* border:1rpx solid black; */
  display: flex;
  overflow-y: scroll;
}
.data_list_2{
  width: 100%;
  /* height: 10000rpx; */
  box-sizing: border-box;
  /* border:1rpx solid black; */
  display: flex;
  flex-direction: column;
  font-size: 24rpx;
  overflow: hidden;
}
.data_lists{
  margin: 20rpx 10rpx;
  width: 100%;
  display: flex;
  height: 60rpx;
  flex-direction: row;
  justify-content: space-between;
  
}
.data_lists>view:nth-child(1){
  word-break:break-all;
  width: 20%;
  height: 80rpx;
  overflow: hidden;
  box-sizing: border-box;
  /* border:1rpx solid black; */
}
.data_lists>view:nth-child(2){
  line-height: 60rpx;
  word-break:break-all;
  width: 20%;
  height: 80rpx;
  overflow: hidden;
  box-sizing: border-box;
  /* border:1rpx solid black; */
}
.data_lists>view:nth-child(3){
  word-break:break-all;
  width: 56%;
  margin-right: 10rpx;
  height: 80rpx;
  overflow: hidden;
  box-sizing: border-box;
  /* border:1rpx solid black; */
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}