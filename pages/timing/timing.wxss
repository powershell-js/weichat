/* pages/timing/timing.wxss */
.conter{
  width: 100%;
  height: 100vh;
  background: #fff;
  overflow: hidden;
}
.center{
  width: 70%;
  height: 60%;
  box-sizing: border-box;
  /* border:2rpx solid red; */
  margin:10% auto;
}
.center>view{
  position: relative;
}
.icon1{
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  right: 0;
  top: 10rpx;
}
.selet_date{
  width: 100%;
  /* height: 100rpx; */
  position: absolute;
  box-sizing: border-box;
  /* border:2rpx solid red; */
  left: 0;
  top: 90rpx;
  background: #fff;
  z-index: 9999;
  box-shadow:0 0 10px #555;
}
.selet_dates{
  width: 100%;
}
.selet_date>view{
  margin: 20rpx 0 10rpx 20rpx;
}
.selet_dates>view{
  margin: 20rpx 0 10rpx 20rpx;
}
.center>view>input{
  width: 100%;
  height: 80rpx;
  box-sizing: border-box;
  border:2rpx solid #008CFF;
  margin: 80rpx 0;
  border-radius: 10rpx;
  padding-left: 20rpx;
  color:#008CFF;
}
button{
  /* margin-top: 300rpx; */
  background: #008CFF;
  color:#fff;
}
.selet_equip{
  overflow-y: scroll;
  width: 100%;
  height: 400rpx;
  position: absolute;
  box-sizing: border-box;
  /* border:2rpx solid red; */
  left: 0;
  top: 90rpx;
  background: #fff;
  z-index: 9999;
  box-shadow:0 0 10px #555;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}